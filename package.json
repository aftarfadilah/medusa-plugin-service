{
  "name": "medusa-service",
  "version": "0.0.1",
  "description": "An plugin for selling services with medusa.",
  "author": "Ai-Ecom <admin@ai-ecom.io>",
  "license": "MIT",
  "scripts": {
    "build": "babel src -d dist --extensions \".ts,.js\"",
    "start": "medusa develop",
    "clean": "./node_modules/.bin/rimraf services/ models/ migrations/ api/ subscribers/ interfaces/ index.js index.map.js",
    "watch": "babel -w src --out-dir . --ignore **/__tests__ --extensions \".ts,.js\"",
    "watch-ts": "tsc --watch",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^1.3.1",
    "medusa-interfaces": "^1.3.0",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.14.5",
    "@medusajs/medusa": "^1.x",
    "@types/node": "^18.11.0",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5"
  },
  "repository": "https://gitlab.com/ai-ecom-io/medusa-service-plugin",
  "keywords": [
    "medusa",
    "service"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "medusa-core-utils": "^1.1.31",
    "medusa-test-utils": "^1.1.37"
  }
}
